      /* Dark mode adjustments */
      @media (prefers-color-scheme: dark) {
        body {
          background-color: #222222 !important;
        }
        .highlight, .highlight table {
          background: #333333 !important;
          background-color: #333333 !important;
        }
        h1, h2, p, a {
          color: #dddddd !important;
        }
      }
