      }
      .edit-region < tr {
        -block-region: true;
      }
      .edit-remove < tr {
        -block-remove: true;
      }
      .button-outlook table tr td {
        mso-padding-alt: 10px 20px !important;
      }
    </mj-style>
    <mj-style>
